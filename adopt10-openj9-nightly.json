{
    "description": "AdoptOpenJDK 10 with Eclipse OpenJ9 JVM (Nightly builds)",
    "homepage": "https://adoptopenjdk.net",
    "version": "201809051700",
    "license": "GPL-2.0-with-classpath-exception",
    "architecture": {
        "64bit": {
            "url": "https://github.com/AdoptOpenJDK/openjdk10-openj9-nightly/releases/download/201809051700/OpenJDK10-OPENJ9_x64_Windows_201809051700.zip",
            "hash": "349c25fb85021d7e9d8ed53e83fa0e67be20a685cabe184caefe4f75ebd80687"
        }
    },
    "extract_to": "tmp",
    "installer": {
        "script": [
            "gci -dir \"$dir\\tmp\" | % { mv \"$dir\\tmp\\$_\\*\" \"$dir\" }",
            "rm -r \"$dir\\tmp\""
        ]
    },
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "url": "https://api.adoptopenjdk.net/v2/info/nightly/openjdk10?openjdk_impl=openj9&os=windows&arch=x64&release=latest&type=jdk",
        "jp": "$.binaries[0].checksum_link",
        "re": "(?<url>.*?(?:(?<year>[\\d]{4})-(?<month>[\\d]{2})-(?<day>[\\d]{2})-(?<hour>[\\d]{2})-(?<minute>[\\d]{2}).*?|(?<year>[\\d]{4})(?<month>[\\d]{2})(?<day>[\\d]{2})(?<hour>[\\d]{2})(?<minute>[\\d]{2}).*?)).sha256.txt",
        "replace": "${year}${month}${day}${hour}${minute}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "$matchUrl.zip"
            }
        },
        "hash": {
            "url": "$matchUrl.sha256.txt",
            "find": "^([a-fA-F0-9]+)\\s"
        }
    }
}
