{
    "description": "Official production-ready open-source builds of OpenJDK 13 (Early Access)",
    "homepage": "http://jdk.java.net/13",
    "version": "13-8-ea",
    "license": "GPL-2.0-with-classpath-exception",
    "architecture": {
        "64bit": {
            "url": "https://download.java.net/java/early_access/jdk13/8/GPL/openjdk-13-ea+8_windows-x64_bin.zip",
            "hash": "f94f40058105735fbb640f8324177cbbec92ec427dcf1572e4485b0a6bbf83cb"
        }
    },
    "extract_dir": "jdk-13",
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "re": "openjdk-(?<major>[u\\d]+)-ea\\+(?<build>[b\\d]+)_windows-x64_bin.(?<zip>tar.gz|zip)",
        "replace": "${major}-${build}-ea"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.java.net/java/early_access/jdk$matchMajor/$matchBuild/GPL/openjdk-$matchMajor-ea+$matchBuild_windows-x64_bin.$matchZip"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    },
    "notes": "This is an Early-access (EA) release."
}
