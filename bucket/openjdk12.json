{
    "description": "Official production-ready open-source builds of OpenJDK 12",
    "homepage": "http://jdk.java.net/12",
    "version": "12-33",
    "license": "GPL-2.0-with-classpath-exception",
    "architecture": {
        "64bit": {
            "url": "https://download.java.net/java/GA/jdk12/33/GPL/openjdk-12_windows-x64_bin.zip",
            "hash": "35a8d018f420fb05fe7c2aa9933122896ca50bd23dbd373e90d8e2f3897c4e92"
        }
    },
    "extract_dir": "jdk-12",
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "re": "/(?<type>early_access|GA)/jdk(?<major>[\\d]+)/(?<build>[\\d]+)/GPL/openjdk-(?:[\\d]+)(?<ea>(-ea)?)(?<eabuild>[+\\d]*)_windows-x64_bin.(?<zip>tar.gz|zip)",
        "replace": "${major}-${build}${ea}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.java.net/java/$matchType/jdk$matchMajor/$matchBuild/GPL/openjdk-$matchMajor$matchEa$matchEabuild_windows-x64_bin.$matchZip"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    }
}
