{
    "description": "AdoptOpenJDK 11 JRE with Eclipse OpenJ9 JVM (Nightly builds)",
    "homepage": "https://adoptopenjdk.net",
    "version": "201905030247",
    "license": "GPL-2.0-with-classpath-exception",
    "architecture": {
        "64bit": {
            "url": "https://github.com/AdoptOpenJDK/openjdk11-binaries/releases/download/jdk11u-2019-05-03-02-47/OpenJDK11U-jre_x64_windows_openj9_2019-05-03-02-47.zip",
            "hash": "4eeb5372b22d82923670915571232126f2d9aaceefd382d6a319f40c29139728"
        }
    },
    "extract_to": "tmp",
    "installer": {
        "script": [
            "gci -dir \"$dir\\tmp\" | % { mv \"$dir\\tmp\\$_\\*\" \"$dir\" }",
            "rm -r \"$dir\\tmp\""
        ]
    },
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "url": "https://api.adoptopenjdk.net/v2/info/nightly/openjdk11?openjdk_impl=openj9&os=windows&arch=x64&release=latest&type=jre",
        "jp": "$.binaries[0].binary_link",
        "re": "https://github.com/(?<url>.*?(?<year>[\\d]{4})-(?<month>[\\d]{2})-(?<day>[\\d]{2})-(?<hour>[\\d]{2})-(?<minute>[\\d]{2}).*?).zip",
        "replace": "${year}${month}${day}${hour}${minute}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/$matchUrl.zip"
            }
        },
        "hash": {
            "url": "$url.sha256.txt",
            "find": "^([a-fA-F0-9]+)\\s"
        }
    }
}
