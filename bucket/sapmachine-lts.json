{
    "description": "SapMachine LTS: An OpenJDK release maintained and supported by SAP",
    "homepage": "https://sap.github.io/SapMachine/",
    "version": "11.0.3",
    "license": "GPL-2.0-with-classpath-exception",
    "architecture": {
        "64bit": {
            "url": "https://github.com/SAP/SapMachine/releases/download//sapmachine-11.0.3/sapmachine-jdk-11.0.3_windows-x64_bin.zip",
            "hash": "dd1299a577fd8f61003eb20baf999a3135ba968f0620f37a477f96afc2c1fc3b"
        }
    },
    "extract_dir": "jdk-$matchMajor+",
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "url": "https://sap.github.io/SapMachine/assets/data/sapmachine_releases.json",
        "jp": "$.assets.11.releases[0].windows-x64",
        "re": "(?<sap>/sapmachine-(?:[\\d.]+)(?:\\%2B|\\+)?(?:[\\d]*)/sapmachine-jdk-(?<version>[\\d.]+)(?:(?<dash>-)(?<ea>ea)\\.(?<build>[\\d]+))?_windows-x64_bin).zip",
        "replace": "${version}${dash}${build}${dash}${ea}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/SAP/SapMachine/releases/download/$matchSap.zip"
            }
        },
        "hash": {
            "url": "https://github.com/SAP/SapMachine/releases/download/$matchSap.sha256.txt",
            "find": "^($sha256)\\s"
        },
        "extract_dir": "jdk-$matchMajor+$matchBuild"
    }
}
