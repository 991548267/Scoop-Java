{
    "description": "Official production-ready open-source builds of OpenJDK 13",
    "homepage": "http://jdk.java.net/13",
    "version": "13-12-ea",
    "license": "GPL-2.0-with-classpath-exception",
    "architecture": {
        "64bit": {
            "url": "https://download.java.net/java/early_access/jdk13/12/GPL/openjdk-13-ea+12_windows-x64_bin.zip",
            "hash": "46275b07fae2cc714c43e423fa5db4c512ccabf3b9eec54efefe4eb848697501"
        }
    },
    "extract_dir": "jdk-13",
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "re": "/(?<type>early_access|GA)/jdk(?<major>[\\d]+)/(?<build>[\\d]+)/(?<random>([a-fA-F0-9]{32}|GPL)?)/(?<file>openjdk-(?<version>[\\d.]+)(?<ea>-ea)?(?:\\+[\\d]+)?_windows-x64_bin.(zip|tar.gz))",
        "replace": "${version}-${build}${ea}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.java.net/java/$matchType/jdk$matchMajor/$matchBuild/$matchRandom/$matchFile"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    }
}
