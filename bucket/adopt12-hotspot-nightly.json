{
    "description": "AdoptOpenJDK 12 with Oracle HotSpot JVM (Nightly builds)",
    "homepage": "https://adoptopenjdk.net",
    "version": "201904031611",
    "license": "GPL-2.0-with-classpath-exception",
    "architecture": {
        "64bit": {
            "url": "https://github.com/AdoptOpenJDK/openjdk12-binaries/releases/download/jdk12u-2019-04-03-16-11/OpenJDK12U-jdk_x64_windows_hotspot_2019-04-03-16-11.zip",
            "hash": "35808a71fec752d0a72174a540e4b578222cdd0414c27cdea343dc5e2c7b8c54"
        }
    },
    "extract_to": "tmp",
    "installer": {
        "script": [
            "gci -dir \"$dir\\tmp\" | % { mv \"$dir\\tmp\\$_\\*\" \"$dir\" }",
            "rm -r \"$dir\\tmp\""
        ]
    },
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "url": "https://api.adoptopenjdk.net/v2/info/nightly/openjdk12?openjdk_impl=hotspot&os=windows&arch=x64&release=latest&type=jdk",
        "jp": "$.binaries[0].binary_link",
        "re": "https://github.com/(?<url>.*?(?<year>[\\d]{4})-(?<month>[\\d]{2})-(?<day>[\\d]{2})-(?<hour>[\\d]{2})-(?<minute>[\\d]{2}).*?).zip",
        "replace": "${year}${month}${day}${hour}${minute}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/$matchUrl.zip"
            }
        },
        "hash": {
            "url": "$url.sha256.txt",
            "find": "^([a-fA-F0-9]+)\\s"
        }
    }
}
