{
    "description": "AdoptOpenJDK 12 with Oracle HotSpot JVM (Nightly builds)",
    "homepage": "https://adoptopenjdk.net",
    "version": "201905021249",
    "license": "GPL-2.0-only WITH Classpath-exception-2.0",
    "architecture": {
        "64bit": {
            "url": "https://github.com/AdoptOpenJDK/openjdk12-binaries/releases/download/jdk12u-2019-05-02-12-49/OpenJDK12U-jdk_x64_windows_hotspot_2019-05-02-12-49.zip",
            "hash": "9b22307fac7a8ec8433f423a4beb9e2daa531afbd240def03a7372f05d56ea43"
        },
        "32bit": {
            "url": "https://github.com/AdoptOpenJDK/openjdk12-binaries/releases/download/jdk12u-2019-05-02-12-49/OpenJDK12U-jdk_x86-32_windows_hotspot_2019-05-02-12-49.zip",
            "hash": "1de6c08107a4a19fabadf682090888af69196629accd8eebea99c4f1d070ed53"
        }
    },
    "extract_to": "tmp",
    "installer": {
        "script": [
            "gci -dir \"$dir\\tmp\" | % { mv \"$dir\\tmp\\$_\\*\" \"$dir\" }",
            "rm -r \"$dir\\tmp\""
        ]
    },
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "url": "https://api.adoptopenjdk.net/v2/info/nightly/openjdk12?openjdk_impl=hotspot&os=windows&arch=x64&release=latest&type=jdk",
        "jp": "$.binaries[0].binary_link",
        "re": "https://github.com/(?<url>.*?jdk(?<major>[\\d]+)u-(?<date>(?<year>[\\d]{4})-(?<month>[\\d]{2})-(?<day>[\\d]{2})-(?<hour>[\\d]{2})-(?<minute>[\\d]{2})))",
        "replace": "${year}${month}${day}${hour}${minute}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/$matchUrl/OpenJDK$matchMajorU-jdk_x64_windows_hotspot_$matchDate.zip"
            },
            "32bit": {
                "url": "https://github.com/$matchUrl/OpenJDK$matchMajorU-jdk_x86-32_windows_hotspot_$matchDate.zip"
            }
        },
        "hash": {
            "url": "$url.sha256.txt",
            "find": "^([a-fA-F0-9]+)\\s"
        }
    }
}
