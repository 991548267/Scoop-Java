{
    "description": "AdoptOpenJDK 12 with Oracle HotSpot JVM (Nightly builds)",
    "homepage": "https://adoptopenjdk.net",
    "version": "201904272322",
    "license": "GPL-2.0-with-classpath-exception",
    "architecture": {
        "64bit": {
            "url": "https://github.com/AdoptOpenJDK/openjdk12-binaries/releases/download/jdk12u-2019-04-27-23-22/OpenJDK12U-jdk_x64_windows_hotspot_2019-04-27-23-22.zip",
            "hash": "a5a678c95a3c2a5bac416e1ed262a7fc3daa641d940ce4acd1b12954ba23813d"
        },
        "32bit": {
            "url": "https://github.com/AdoptOpenJDK/openjdk12-binaries/releases/download/jdk12u-2019-04-27-23-22/OpenJDK12U-jdk_x86-32_windows_hotspot_2019-04-27-23-22.zip",
            "hash": "08c840965aab93285a5b2651ad65af21cda6eedd51abdd90205a087abeaa61ce"
        }
    },
    "extract_to": "tmp",
    "installer": {
        "script": [
            "gci -dir \"$dir\\tmp\" | % { mv \"$dir\\tmp\\$_\\*\" \"$dir\" }",
            "rm -r \"$dir\\tmp\""
        ]
    },
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "url": "https://api.adoptopenjdk.net/v2/info/nightly/openjdk12?openjdk_impl=hotspot&os=windows&arch=x64&release=latest&type=jdk",
        "jp": "$.binaries[0].binary_link",
        "re": "https://github.com/(?<url>.*?jdk(?<major>[\\d]+)u-(?<date>(?<year>[\\d]{4})-(?<month>[\\d]{2})-(?<day>[\\d]{2})-(?<hour>[\\d]{2})-(?<minute>[\\d]{2})))",
        "replace": "${year}${month}${day}${hour}${minute}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/$matchUrl/OpenJDK$matchMajorU-jdk_x64_windows_hotspot_$matchDate.zip"
            },
            "32bit": {
                "url": "https://github.com/$matchUrl/OpenJDK$matchMajorU-jdk_x86-32_windows_hotspot_$matchDate.zip"
            }
        },
        "hash": {
            "url": "$url.sha256.txt",
            "find": "^([a-fA-F0-9]+)\\s"
        }
    }
}
