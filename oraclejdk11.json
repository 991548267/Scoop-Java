{
    "description": "Oracle JDK 11",
    "homepage": "https://www.oracle.com/technetwork/java/javase/overview/index.html",
    "version": "11-28",
    "license": "https://www.oracle.com/technetwork/java/javase/terms/license/javase-license.html",
    "architecture": {
        "64bit": {
            "url": "http://download.oracle.com/otn-pub/java/jdk/11+28/55eed80b163941c8885ad9298e6d786a/jdk-11_windows-x64_bin.zip",
            "hash": "d64b9d725f0ed096ae839ef8506deb3b6f509b2e3ee1f9b0792c5116623d4c9d"
        }
    },
    "cookie": {
        "oraclelicense": "accept-securebackup-cookie"
    },
    "extract_dir": "jdk-11",
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "url": "https://www.oracle.com/technetwork/java/javase/downloads/jdk11-downloads-5066655.html",
        "re": "otn-pub/java/jdk/(?<path>(?<major>[\\d.]+)\\+(?<build>[\\d]+)/(?<hash>[a-fA-F0-9]{32})/jdk-(?:.*?)_windows-x64_bin.zip)",
        "replace": "${major}-${build}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "http://download.oracle.com/otn-pub/java/jdk/$matchPath"
            }
        },
        "hash": {
            "url": "https://www.oracle.com/technetwork/java/javase/downloads/jdk11-downloads-5066655.html",
            "find": "$basename.*([a-fA-F0-9]{64})\"};"
        },
        "extract_dir": "jdk-$matchMajor"
    }
}
