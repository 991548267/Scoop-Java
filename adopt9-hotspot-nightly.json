{
    "description": "AdoptOpenJDK 9 with Oracle HotSpot JVM (Nightly builds)",
    "homepage": "https://adoptopenjdk.net",
    "version": "201809071400",
    "license": "GPL-2.0-with-classpath-exception",
    "architecture": {
        "64bit": {
            "url": "https://github.com/AdoptOpenJDK/openjdk9-nightly/releases/download/201809071400/OpenJDK9_x64_Windows_201809071400.zip",
            "hash": "6442b15a1af6d8bd3170c151db2d853af76b2f46cf11d8d45b3d31d6ee9f41f5"
        }
    },
    "extract_to": "tmp",
    "installer": {
        "script": [
            "gci -dir \"$dir\\tmp\" | % { mv \"$dir\\tmp\\$_\\*\" \"$dir\" }",
            "rm -r \"$dir\\tmp\""
        ]
    },
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "url": "https://api.adoptopenjdk.net/v2/info/nightly/openjdk9?openjdk_impl=hotspot&os=windows&arch=x64&release=latest&type=jdk",
        "jp": "$.binaries[0].checksum_link",
        "re": "https://github.com/(?<url>.*?(?:(?<year>[\\d]{4})-(?<month>[\\d]{2})-(?<day>[\\d]{2})-(?<hour>[\\d]{2})-(?<minute>[\\d]{2}).*?|(?<year>[\\d]{4})(?<month>[\\d]{2})(?<day>[\\d]{2})(?<hour>[\\d]{2})(?<minute>[\\d]{2}).*?)).sha256.txt",
        "replace": "${year}${month}${day}${hour}${minute}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/$matchUrl.zip"
            }
        },
        "hash": {
            "url": "https://github.com/$matchUrl.sha256.txt",
            "find": "^([a-fA-F0-9]+)\\s"
        }
    }
}
